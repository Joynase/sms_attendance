*.sublime-project

# Ignore logs and temp files
*.log
*.tmp

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore vendor (if using Composer)
vendor/

# Ignore node_modules (if using frontend tools)
node_modules/

# Ignore cache
cache/
*.cache

# Ignore uploads or user-generated content
uploads/
storage/
